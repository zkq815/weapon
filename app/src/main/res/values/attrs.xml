    <declare-styleable name="MyProgress">
        <attr name="title_string" format="string"/>
        <attr name="title_color" format="color"/>
        <attr name="title_size" format="dimension"/>
        <attr name="circle_color" format="color"/>
        <attr name="ring_like_color" format="color"/>
        <attr name="first_color" format="color"/>
        <attr name="second_color" format="color"/>
        <attr name="third_color" format="color"/>
        <attr name="fourth_color" format="color"/>
        <attr name="fifth_color" format="color"/>
        <attr name="sixth_color" format="color"/>
        <attr name="seventh_color" format="color"/>
    </declare-styleable>
</resources>